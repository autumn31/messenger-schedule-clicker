{
  "name": "doc filler - chang gung",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/mess16.png",
      "32": "/images/mess32.png",
      "48": "/images/mess64.png",
      "128": "/images/mess128.png"
    }
  },
  "icons": {
    "16": "/images/mess16.png",
    "32": "/images/mess32.png",
    "48": "/images/mess64.png",
    "128": "/images/mess128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.messenger.com/*"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 3
}